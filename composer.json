{
    "name": "cubequence/request",
    "description": "A simplified Guzzle client for CubeQuence",
    "keywords": [
        "guzzle",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "dev-master",
        "pestphp/pest": "^1.0"
    },
    "scripts": {
        "tests": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\Request\\": "src/Request"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CubeQuence/Request/issues",
        "source": "https://github.com/CubeQuence/Request"
    }
}
